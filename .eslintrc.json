{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "generator-star-spacing": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "prefer-arrow-callback": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "max-len": 0,
    "comma-dangle": 0,
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ]
  }
}
